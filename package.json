{
  "name": "@2060-digital/fullerton",
  "private": true,
  "prettier": "@2060-digital/prettier-config",
  "scripts": {
    "dev": "NEXT_PUBLIC_SITE_URL=$URL next dev",
    "build": "NEXT_PUBLIC_SITE_URL=$URL next build",
    "postbuild": "NEXT_PUBLIC_SITE_URL=$URL next-sitemap && npm run setup-search",
    "setup-search": "npx --yes pagefind@1.0.5-rc2 --site \".next\" && rm -rf public/pagefind && mv .next/pagefind public/pagefind",
    "start": "NEXT_PUBLIC_SITE_URL=$URL next start",
    "lint": "NEXT_PUBLIC_SITE_URL=$URL next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/lato": "^5.0.18",
    "@fontsource/playfair-display": "^5.0.18",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "axios": "^1.6.4",
    "classnames": "^2.5.1",
    "fuse.js": "^7.0.0",
    "lodash": "^4.17.21",
    "lodash.isobject": "^3.0.2",
    "maplibre-gl": "^4.0.0",
    "next": "13.4.19",
    "next-sitemap": "^4.2.3",
    "next-usequerystate": "^1.17.0",
    "outdent": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.2",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "storyblok-js-client": "^6.4.1",
    "storyblok-react": "^0.1.2",
    "storyblok-rich-text-react-renderer": "^2.9.1"
  },
  "devDependencies": {
    "@2060-digital/prettier-config": "1.0.1",
    "@babel/core": "^7.23.7",
    "@plaiceholder/next": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "filenamify": "^6.0.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "husky": "8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "15.2.0",
    "plaiceholder": "^3.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,jsx,css,md}": "prettier --write"
  }
}
